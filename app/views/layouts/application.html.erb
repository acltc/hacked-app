<!DOCTYPE html>
<html>
  <head>
    <title>QCH</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link href="/css/old-bootstrap.css" rel="stylesheet" type="text/css" media="all" />
    <link href="/css/bootstrap-theme.css" rel="stylesheet" type="text/css" media="all" />
    
  </head>

  <body style="visibility: hidden">

    <!-- Navbar
    ================================================== -->
    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          </button>
          <a class="brand" href="/">QCH</a>
          <div class="nav-collapse collapse">
            <ul class="nav">
              <li class="">
                <a href="/">Main Terminal</a>
              </li>
              <li class="">
                <a href="/phishing">Phishing</a>
              </li>
              <li class="">
                <a href="/cyber-warfare">Cyber Warfare</a>
              </li>
              <li class="">
                <a href="/ransomware">Ransomware</a>
              </li>
              <li class="">
                <a href="/virus">Virus Development</a>
              </li>
              <li class="">
                <a href="#">Nuclear Program</a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>

    <div class="container">

      <%= yield %>

    </div>

    <script src="/js/jquery-3.2.1.min.js"></script>
    <script src="/js/bootstrap.js"></script>
    <% if @disable_386 %>
      <script type="text/javascript">_386 = { fastLoad: true };</script>
    <% else %>
      <script type="text/javascript">_386 = { onePass: true, speedFactor: 0.5 };</script>
    <% end %>
  </body>
</html>
